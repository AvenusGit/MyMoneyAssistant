services:
  mymoneyassistant:
    build:
      args:
        BUILD_CONFIGURATION: Release
      context: C:\Projects\PET\MyMoneyAssistant
      dockerfile: MyMoneyAssistant/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: MyMoneyAssistant
    container_name: MyMoneyAssistant_1
    depends_on:
      postgres:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORTS: '8081'
      ASPNETCORE_HTTP_PORTS: '8080'
    image: mymoneyassistant
    labels:
      com.microsoft.visual-studio.project-name: MyMoneyAssistant
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /.nuget/packages
        --additionalProbingPath /.nuget/fallbackpackages --additionalProbingPath /.nuget/fallbackpackages2  "/app/MyMoneyAssistant.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8000
      target: 80
    - target: 8080
    - target: 8081
    tty: true
    volumes:
    - C:\Program Files\Microsoft Visual Studio\2022\Community\Common7\IDE\CommonExtensions\Microsoft\HotReload:/HotReloadAgent:ro
    - C:\Program Files\Microsoft Visual Studio\2022\Community\MSBuild\Sdks\Microsoft.Docker.Sdk\tools\HotReloadProxy\linux-x64\net6.0:/HotReloadProxy:ro
    - C:\Program Files\Microsoft Visual Studio\2022\Community\MSBuild\Sdks\Microsoft.Docker.Sdk\tools\TokenService.Proxy\linux-x64\net6.0:/TokenService.Proxy:ro
    - C:\Users\v.brodikov\AppData\Roaming\ASP.NET\Https:/home/app/.aspnet/https:ro
    - C:\Users\v.brodikov\AppData\Roaming\Microsoft\UserSecrets:/home/app/.microsoft/usersecrets:ro
    - C:\Users\v.brodikov\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\v.brodikov\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\v.brodikov\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
  postgres:
    container_name: postgres
    environment:
      POSTGRES_PASSWORD: postgres
    image: postgres
version: '3.4'